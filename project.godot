window/size/viewport_height=270
window/size/window_width_override=2500
window/size/window_height_override=1500
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/resonate/plugin.cfg")

